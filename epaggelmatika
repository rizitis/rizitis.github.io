    <script>
        (function ($) {
            $(document).ready(function () {
                $("#clearFilters").on("click", function () {
                    $('#company').selectpicker('val', ["ELPEDISON","NRG","OTE ESTATE","PROTERGIA","SOLAR ENERGY","VOLTERRA","VOLTON ","WE ENERGY","ΔΕΗ","ΕΛΙΝΟΙΛ","ΖΕΝΙΘ","ΗΡΩΝ","ΦΥΣΙΚΟ ΑΕΡΙΟ ΕΛΛΗΝΙΚΗ ΕΤΑΙΡΙΑ ΕΝΕΡΓΕΙΑΣ"]);
                    $('#color').selectpicker('val', ['Ειδικό Τιμολόγιο', 'Σταθερό Τιμολόγιο', 'Κυμαινόμενο Τιμολόγιο', 'Δυναμικό Τιμολόγιο',]);
                    $('#month').selectpicker('val', ['1']);
                    $('#year').selectpicker('val', ['2024']);
                    filterRows();
                });

                function filterRows() {
                    var company_column_dropdown = $('#company').val();
                    var color_column_dropdown = $('#color').val();
                    var year_column_dropdown = $('#year').val();
                    var month_column_dropdown = $('#month').val();

                    $("#billing_table tbody tr").each(function () {
                        var company_column = $(this).find("td:first").text();
                        var color_column = $(this).find("td:nth-child(4)").data('filter-value');
                        var year_column = $(this).find("td:nth-child(2)").text();
                        var month_column = $(this).find("td:nth-child(3)").text();

                        var isInCompanyDropdown = company_column_dropdown.includes(company_column);
                        var isInColorDropdown = color_column_dropdown.includes(color_column);
                        var isInYearDropdown = year_column_dropdown.includes(year_column);
                        var isInMonthDropdown = month_column_dropdown.includes(month_column);

                        $(this).hide();
                        if(isInCompanyDropdown && isInColorDropdown && isInYearDropdown && isInMonthDropdown){
                            $(this).show();
                        }
                    });
                }

                $('#company').on('change', filterRows);
                $('#color').on('change', filterRows);
                $('#month').on('change', filterRows);
                $('#year').on('change', filterRows);

                $('#company').selectpicker('val', ["ELPEDISON","NRG","OTE ESTATE","PROTERGIA","SOLAR ENERGY","VOLTERRA","VOLTON ","WE ENERGY","ΔΕΗ","ΕΛΙΝΟΙΛ","ΖΕΝΙΘ","ΗΡΩΝ","ΦΥΣΙΚΟ ΑΕΡΙΟ ΕΛΛΗΝΙΚΗ ΕΤΑΙΡΙΑ ΕΝΕΡΓΕΙΑΣ"]);
                $('#color').selectpicker('val', ['Ειδικό Τιμολόγιο', 'Σταθερό Τιμολόγιο', 'Κυμαινόμενο Τιμολόγιο', 'Δυναμικό Τιμολόγιο',]);
                $('#month').selectpicker('val', ['1']);
                $('#year').selectpicker('val', ['2024']);
                filterRows();

                var billing_table = $('#billing_table').DataTable({
                    "searching": false,
                    "info": false,
                    "paging": false,
                    "order": [[ 8, 'asc' ]]
                });

                $('#checkbox_ekptosi').click(function () {
                    if ($(this).is(':checked')) {
                        $('.checkbox_ekptosi_on').show();
                        $('.checkbox_ekptosi_off').hide();
                        billing_table.order([[8, 'asc']]).draw();
                    } else {
                        $('.checkbox_ekptosi_on').hide();
                        $('.checkbox_ekptosi_off').show();
                        billing_table.order([[5, 'asc']]).draw();

                    }
                });
            });
        })(jQuery);
    </script>

    				<!-- wp:shortcode -->

